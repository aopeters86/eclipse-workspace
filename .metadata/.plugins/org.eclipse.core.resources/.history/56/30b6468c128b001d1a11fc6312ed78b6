class Positive{

	  public static int sum(int[] arr){
		  
	    
	    int posArr[] = arr;
	    int positive = posArr[0];
	    int negative = posArr[0];
	    
	    
//	    int negative = posArr[];
	    
	    for(int i = 0; i < posArr.length; i++) {
	    	if (i > 0) {
	    		positive += posArr[i];
	    	}
	    	if(i < 0) {
	    		negative += posArr[i];
	    	}
	    	
	    }
	       
	    
	    
	    if(arr.length==0){
	    	return 0;
	  }
		return 0;
	  }
	}