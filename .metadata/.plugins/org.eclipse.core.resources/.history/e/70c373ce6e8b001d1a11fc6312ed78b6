
public class SumOfPositive {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int test[] = {-2,10,-3,10};
		
		System.out.println("Sum: " + sum(test));
		
}



public static int sum(int[] arr) {
int positive = 0;

if(arr.length==0){
	return 0;
}

for(int i = 0; i < arr.length; i++) {
	if (i >= 0) {
		positive += i;
		
	}
	
}    

return positive;

}
}
