import java.util.Scanner;
public class arraysBasic {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		
		int age[] = new int[3];
		
		age[0] = 0;
		age[1] = 1;
		age[2] = 2;
		
		System.out.println(age[0]);
		System.out.println(age[1]);
		System.out.println(age[2]);

	
	
	
	}
	


}
